package main

import (
    "github.com/ramoncl001/comet
    "{{.Module}}/controllers"
    "{{.Module}}/services"
)

func main() {
    app := comet.CreateServer()

    app.UseMiddleware(middleware.RequestLogging)

    comet.RegisterTransient[services.FooService](services.NewFooService)

    app.MapController(controllers.NewFooController)

    app.Run(":8080")
}